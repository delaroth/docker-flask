pipeline {
    agent any

    stages {
        stage('echo') {
            steps {
                echo 'hi shahar'
            }

        }
        stage('docker build'){
             steps{
          sh 'docker build -t delaroth/new-image'
          sh 'docker login -u $DOCKERHUB_USER -p livevil123'
          sh 'docker push delaroth/new-image'
}
    }
}
}
